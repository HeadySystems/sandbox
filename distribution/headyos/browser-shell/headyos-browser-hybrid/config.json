{
  "name": "HeadyOS Browser (Hybrid)",
  "version": "0.1.0",
  "mode": "hybrid",
  "description": "Chromium-based browser with HeadyOS â€” local preferred, cloud fallback",
  "api": {
    "endpoint": "http://manager.dev.local.headysystems.com:3300",
    "fallback": "https://api.heady.systems",
    "model_runner": "https://api.headysystems.com",
    "cloud_model_endpoint": "https://models.heady.systems"
  },
  "features": {
    "side_panel": true,
    "task_helper": true,
    "tab_intelligence": true,
    "page_actions": true,
